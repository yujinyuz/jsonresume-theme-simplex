{{#if resume.projects.length}}
	{{#each resume.projects}}
		<section class="position">
			{{#if @first}}
				<h2>Projects</h2>
			{{/if}}
			<h3>
				{{#if url}}
					<a target="_blank" rel="noopener" href="{{url}}">{{name}}</a>
				{{else}}
					{{name}}
				{{/if}}
			</h3>

			{{#if releaseDate}}
				<h4>
					{{ date releaseDate "MMM, YYYY" datejs=false }}
				</h4>
			{{/if}}

			{{#if description}}
				<p>{{description}}</p>
			{{/if}}
		</section>
	{{/each}}
{{/if}}
